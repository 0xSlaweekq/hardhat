{
  "name": "hardhat",
  "version": "0.9.73",
  "description": "This project demonstrates a basic Hardhat use case. It comes with a sample contract, a test for that contract, and a script that deploys that contract.",
  "homepage": "https://github.com/NarcoNik/hardhat#readme",
  "bugs": {
    "url": "https://github.com/NarcoNik/hardhat/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NarcoNik/hardhat.git"
  },
  "license": "MIT",
  "author": "slaweekq",
  "main": "index.js",
  "directories": {
    "build": "build",
    "contracts": "contracts",
    "deployments": "deployments",
    "scripts": "scripts",
    "tests": "tests"
  },
  "scripts": {
    "auto-test": "slither .",
    "clean": "rm -rf node_modules package-lock.json",
    "compile": "npx hardhat clean && npx hardhat compile",
    "coverage:Profit": "npx hardhat coverage ./tests/Profit.test.ts --network localhost --show-stack-traces",
    "deploy:hardhat": "npx hardhat run scripts/deploy.ts --network hardhat",
    "deploy:local": "npx hardhat run scripts/deploy.ts --network localhost --show-stack-traces",
    "deploy:tenderly": "npx hardhat run scripts/deploy.ts --network tenderly --show-stack-traces",
    "format": "npx prettier --write ./ && npx eslint --fix ./",
    "gui": "npx hardhat gui",
    "lint": "npx solhint contracts/**/**/**/**/**/**/*.sol",
    "linter": "npx solhint **/*.sol",
    "ln": "docker-compose -f ./docker-compose.yml up -d",
    "ln-reboot": "docker-compose -f ./docker-compose.yml down -v && docker-compose -f ./docker-compose.yml rm -v && docker-compose -f ./docker-compose.yml up -d",
    "ln-stop": "docker-compose -f ./docker-compose.yml down -v && docker-compose -f ./docker-compose.yml rm -v",
    "metrics": "npx solidity-code-metrics contracts/**/**/**/**/**/**/*.sol > Docs/MetricsAllContracts.md && npx solidity-code-metrics contracts/**/**/**/**/**/**/*.sol --html > Docs/MetricsAllContracts.html",
    "metrics:html": "npx solidity-code-metrics contracts/**/**/**/**/**/**/*.sol --html > Docs/MetricsAllContracts.html",
    "metrics:md": "npx solidity-code-metrics contracts/**/**/**/**/**/**/*.sol > Docs/MetricsAllContracts.md",
    "push": "git add ./ && git commit -m 'refactoring' -a && git push origin",
    "test": "npx hardhat test --network localhost",
    "test:Profit": "npx hardhat test ./tests/Profit.test.ts --network localhost --show-stack-traces",
    "verify": "npx hardhat run scripts/verify.ts",
    "verify:tenderly": "npx hardhat tenderly:verify --network tenderly"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run format"
    }
  },
  "dependencies": {
    "@truffle/hdwallet-provider": "^1.5.1",
    "@uniswap/v2-sdk": "^3.2.0",
    "@uniswap/v3-sdk": "^3.10.0",
    "ganache-core": "^2.13.2",
    "hardhat": "^2.17.0",
    "truffle-plugin-verify": "^0.5.15"
  },
  "devDependencies": {
    "@chainlink/contracts": "^0.8.0",
    "@nomiclabs/buidler": "1.4.8",
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-etherscan": "3.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "4.9.2",
    "@openzeppelin/contracts-upgradeable": "4.9.2",
    "@openzeppelin/hardhat-upgrades": "1.28.0",
    "@openzeppelin/test-helpers": "0.5.16",
    "@solidstate/contracts": "^0.0.58",
    "@sushiswap/contracts": "^1.0.0-canary.0",
    "@sushiswap/core": "^1.4.2",
    "@typechain/ethers-v5": "7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "4.2.21",
    "@types/jest": "^27.0.3",
    "@types/mocha": "9.0.0",
    "@types/node": "16.9.0",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.3",
    "chai": "^4.3.4",
    "dotenv": "^16.0.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.7.2",
    "ganache-cli": "^6.12.2",
    "hardhat-abi-exporter": "^2.4.0",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-deploy": "0.11.4",
    "hardhat-gas-reporter": "1.0.8",
    "husky": "^9.0.11",
    "path": "^0.12.7",
    "prettier": "^3.2.5",
    "prettier-plugin-packagejson": "^2.4.11",
    "prettier-plugin-solidity": "^1.3.1",
    "solhint": "^4.1.1",
    "solhint-community": "^3.7.0",
    "solhint-plugin-prettier": "^0.1.0",
    "solidity-code-metrics": "^0.0.25",
    "solidity-coverage": "^0.8.7",
    "solidity-metrics": "^0.0.5",
    "standard-version": "^9.3.0",
    "ts-generator": "0.1.1",
    "ts-node": "10.2.1",
    "typechain": "5.1.2",
    "typescript": "4.4.4",
    "web3": "^4.3.0"
  },
  "engines": {
    "node": ">=18.12.0"
  }
}
